    "def swap_case(s):\n",
    "    result=''\n",
    "    for i in s: \n",
    "        if i.islower(): \n",
    "            result += i.upper()\n",
    "        else: \n",
    "            result +=i.lower()\n",
    "    return result\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    s = input()\n",
    "    result = swap_case(s)\n",
    "    print(result)"
